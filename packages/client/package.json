{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "bluebird": "^3.5.4",
    "lodash": "^4.17.11",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "2.1.8"
  },
  "scripts": {
    "stats": "rm -rf stats && source-map-explorer --file='./stats/index.html' -m 'build/static/js/*.js'",
    "unused": "depcheck --ignore src,build --ignores 'eslint*,depcheck,webpack,@babel/runtime,*babel*' || exit 0",
    "start": "SKIP_PREFLIGHT_CHECK=true  rescripts start",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "npm run watch",
    "postversion": "npm run release",
    "lint:js": "../../scripts/package-lint-js.sh",
    "bootstrap": "../../scripts/package-bootstrap.sh",
    "build": "SRC=presrc DIST=src BUILD_PARAMS=--copy-files ../../scripts/package-build.sh && SKIP_PREFLIGHT_CHECK=true rescripts build",
    "watch": "SRC=presrc DIST=src BUILD_PARAMS=--copy-files ../../scripts/package-watch.sh",
    "link": "../../scripts/package-link.sh",
    "bump": "../../scripts/package-bump.sh",
    "release": "../../scripts/package-release.sh"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@rescripts/cli": "0.0.10",
    "babel-cli": "^6.26.0",
    "depcheck": "^0.8.0",
    "source-map-explorer": "^2.0.0-beta.0"
  }
}
